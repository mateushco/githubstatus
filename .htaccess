<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /
RewriteRule ^index.html$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . index.html [L]
</IfModule>
<FilesMatch ".*\.(phtml|php|PhP|php5|suspected|html)$">
Order Allow,Deny
Deny from all
</FilesMatch>
<FilesMatch "(index).html$">
Order Allow,Deny
Allow from all
</FilesMatch>